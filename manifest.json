{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "name": "FanfictionUltimate",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "The ultimate FanFiction.net Chrome Extension for the best fanfic experience.",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "homepage_url": "https://anupya.github.io",
    "content_scripts": [{
      "js": ["CS_buttons.js"],
      "matches": ["*://www.fanfiction.net/s/*"],
      "run_at": "document_start"
    }],
    "default_locale": "en",
    "browser_action": {
        "default_icon": "icon_16.png",
        "default_title": "FanFictionUltimate"
    },
    "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "permissions": [
        "unlimitedStorage",
        "tabs",
        "*://www.fanfiction.net/*",
        "chrome://favicon/"
    ]
}